<h1>Formulario para Agregar Producto</h1>
<form id="agregarForm">
    <label for="producto">Producto:</label>
    <input id="producto" name="producto" required>

    <button type="submit">Agregar Producto</button>
</form>

<h1>Formulario para Eliminar Producto por ID</h1>
<form id="eliminarForm">
    <label for="productId">ID del Producto a Eliminar:</label>
    <input type="text" id="productId" name="productId" required>

    <button type="submit">Eliminar Producto</button>
</form>

<div style="display:flex; gap: 5rem;">
  <div>

    <h1 id="ola">Productos agregados</h1>
    <div id="container">
      <div>
    {{#each products}}
    <ul>
        <li>TITULO: {{this.titulo}}</li>
        <li>DESCRIPCION: ${{this.descripcion}}</li>
        <li>code: ${{this.code}}</li>
        <li>PRECIO: ${{this.precio}}</li>
        <li><img src="{{this.thumbnail}}`"> </li>
        <li>stock: ${{this.stock}}</li>
        <li>Categoria: {{this.category}}</li>
        <li>ID: {{this.id}}</li>
    </ul>
    {{/each}}
    </div>
    </div>
  </div>
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>